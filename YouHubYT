local FrameTimer = tick()
local FrameCounter = 0;
local FPS = 60;

local WatermarkConnection = game:GetService('RunService').RenderStepped:Connect(function()
	FrameCounter += 1;

	if (tick() - FrameTimer) >= 1 then
		FPS = FrameCounter;
		FrameTimer = tick();
		FrameCounter = 0;
	end;

	v0:SetWatermark(('You Hub | %s fps | %s ms'):format(
		math.floor(FPS),
		math.floor(game:GetService('Stats').Network.ServerStatsItem['Data Ping']:GetValue())
	));
end);

v0.KeybindFrame.Visible = true; -- todo: add a function for this

v0:OnUnload(function()
	WatermarkConnection:Disconnect()

	print('Unloaded!')
	v0.Unloaded = true
end)
-- UI Settings
local WTF4 = UI:AddTab("Ui Setting")
local MenuGroup = WTF4:AddLeftGroupbox('Menu')

-- I set NoUI so it does not show up in the keybinds menu
MenuGroup:AddButton('Unload Library', function() Library:Unload() end)
MenuGroup:AddLabel('Menu bind'):AddKeyPicker('MenuKeybind', { Default = 'End', NoUI = true, Text = 'Menu keybind' })

v0.ToggleKeybind = Options.MenuKeybind -- Allows you to have a custom keybind for the menu

-- Addons:
-- SaveManager (Allows you to have a configuration system)
-- ThemeManager (Allows you to have a menu theme system)

-- Hand the library over to our managers
v1:SetLibrary(v0)
v2:SetLibrary(v0)

-- Ignore keys that are used by ThemeManager.
-- (we dont want configs to save themes, do we?)
v2:IgnoreThemeSettings()

-- Adds our MenuKeybind to the ignore list
-- (do you want each config to have a different menu key? probably not.)
v2:SetIgnoreIndexes({ 'MenuKeybind' })

-- use case for doing it this way:
-- a script hub could have themes in a global folder
-- and game configs in a separate folder per game
v1:SetFolder('MyScriptHub')
v2:SetFolder('MyScriptHub/specific-game')

-- Builds our config menu on the right side of our tab
v2:BuildConfigSection(WTF4)

-- Builds our theme menu (with plenty of built in themes) on the left side
-- NOTE: you can also call v1:ApplyToGroupbox to add it to a specific groupbox
v1:ApplyToTab(WTF4)

-- You can use the v2:LoadAutoloadConfig() to load a config
-- which has been marked to be one that auto loads!
v2:LoadAutoloadConfig()























